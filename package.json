    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "knackhq-client": "git+https://github.com/john-raymon/knackhq-client.git",
    "mongoose": "^5.4.17",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
